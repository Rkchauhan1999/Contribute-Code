
public class Demo {

	public static void main(String[] args) {

		int x = 343;
		int r = 0;
		int nn = 0;
		int temp = x;
		while (x > 0) {
			r = x % 10;
			nn = (nn * 10) + r;
			x = x / 10;
		}
		if (temp == nn) {
			System.out.println("Yes!.....");
		} else
			System.out.println("No!....");
			
///**************************************************************			
			
			
			
	}
}
